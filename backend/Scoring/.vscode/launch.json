        {
            "name": ".NET Core Launch (Bank web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/IntelART.Ameria.BankModuleWebApp/bin/Debug/netcoreapp1.1/IntelART.Ameria.BankModuleWebApp.dll",
            "args": [],
            "cwd": "${workspaceFolder}/IntelART.Ameria.BankModuleWebApp",
            "stopAtEntry": false,
            "internalConsoleOptions": "openOnSessionStart",
            "launchBrowser": {
                "enabled": true,
                "args": "${auto-detect-url}",
                "windows": {
                    "command": "cmd.exe",
                    "args": "/C start ${auto-detect-url}"
                },
                "osx": {
                    "command": "open"
                },
                "linux": {
                    "command": "xdg-open"
                }
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development",
                "ASPNETCORE_URLS": "http://*:5004"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        },
